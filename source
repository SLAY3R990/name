OrionLib:MakeNotification({
	Name = "Logged In!",
	Content = "You need to type in the key now, "..Player.Name..".",
	Image = "rbxassetid://4483345998",
	Time = 15
}) 

getgenv().Key = "SLAY3RISCOOL" 
getgenv().KeyInput = "string" --Require For The Key To Work

local Tab = Window:MakeTab({
	Name = "Key",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddTextbox({
	Name = "Enter Key Here",
	Default = "",
	TextDisappear = false,
	Callback = function(Value)
		getgenv().KeyInput = Value
	end	  
})

Tab:AddButton({
    Name = "Check Key",
    Callback = function()
        if getgenv().KeyInput == getgenv().Key then
            OrionLib:MakeNotification({
                Name = "Checking Key",
                Content = "Checking The Key You Entered...",
                Image = "rbxassetid://4483345998",
                Time = 5
            })
            wait(2)
            OrionLib:MakeNotification({
                Name = "Correct Key!",
                Content = "The key you entered is Correct.",
                Image = "rbxassetid://4483345998",
                Time = 10
            })
            wait(1)
            OrionLib:Destroy()
            wait(.3)
            MakeScriptHub()
        else
           OrionLib:MakeNotification({
                Name = "Checking Key",
                Content = "Checking The Key You Entered...",
                Image = "rbxassetid://4483345998",
                Time = 5
            })
            wait(2)
            OrionLib:MakeNotification({
                Name = "Incorrect Key!",
                Content = "The key you entered is incorrect.",
                Image = "rbxassetid://4483345998",
                Time = 10
            })
        end
    end
})

Tab:AddButton({
	Name = "Copy Key Link",
	Callback = function()
      		setclipboard("https://social-unlock.com/BUdXr")
  	end    
}) 
    
OrionLib:Init()
